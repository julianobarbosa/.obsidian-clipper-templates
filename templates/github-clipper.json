{
	"schemaVersion": "0.1.0",
	"name": "GitHub",
	"behavior": "create",
	"noteContentFormat": "{{content}}",
	"properties": [
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "published",
			"value": "{{schema:uploadDate|date:YYYY-MM-DD}}",
			"type": "date"
		},
		{
			"name": "image",
			"value": "{{schema:thumbnailUrl|slice:0}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "[github, project]",
			"type": "multitext"
		},
		{
			"name": "note",
			"value": "{{noteName}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://github.com"
	],
	"noteNameFormat": "{{schema:author}}â€“{{schema:name}}",
	"path": "06 - daily/_inbox/clippings"
}